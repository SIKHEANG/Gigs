<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @musician.name %>
</p>

<%= image_tag @musician.image.url(:medium) %>

<p>
  <strong>Description:</strong>
  <%= @musician.description %>
</p>

<p>
  <strong>Genre:</strong>
  <%= @musician.genre %>
</p>

